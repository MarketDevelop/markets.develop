# short time
msgid "g:i A"
msgstr "G:i"

# long time
msgid "g:i:s A"
msgstr "G:i:s"

# short date
msgid "n/j/Y"
msgstr "Y/m/d"

# long date
msgid "l, M d, Y"
msgstr "Y年n月j日"

# short date time
msgid "n/j/Y g:i:s A"
msgstr "Y/m/d G:i"

# long date time
msgid "l, M d, Y, g:i:s A"
msgstr "Y年n月j日 G:i:s"
